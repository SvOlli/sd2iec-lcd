CONFIG_REMOTE_DISPLAY=n
CONFIG_M2I=n
CONFIG_P00CACHE=n
NAME_OVERRIDE=mini
CONFIG_HAVE_EEPROMFS=n
CONFIG_LOADER_ANOTHERWORLD=n
CONFIG_LOADER_WINGSOFFURY=n
